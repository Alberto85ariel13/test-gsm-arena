{
  "name": "test-gsm-arena-service",
  "private": true,
  "version": "1.1.0",
  "main": "src/index.js",
  "author": "Alberto Delgado <alberto85ariel13@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/bin/start.js",
    "predev": "yarn doc",
    "dev": "nodemon src/index.ts --watch 'src' --ext 'ts' --ignore 'src/**/*.spec.ts'",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "lint-fix": "yarn lint --fix",
    "test": "yarn docker-down && yarn docker-build && yarn docker-run && yarn docker-down",
    "test-all": "yarn test-pre 'tests/**/*.spec.ts' --exit",
    "test-ci": "nyc -r html -r text -r lcovonly --report-dir=test/coverage mocha -r 'tests/index.spec.js' -R mocha-junit-reporter -O mochaFile=./test/test-results.xml 'tests/**/*.spec.ts' --exit",
    "test-coverage": "nyc yarn test-all",
    "test-functional": "yarn test-pre 'tests/functional/**/*.spec.ts' --watch-extensions ts --exit",
    "test-integration": "yarn test-pre 'tests/integration/**/*.spec.ts' --watch-extensions ts --exit",
    "test-unit": "yarn test-pre 'tests/unit/**/*.spec.ts' --watch-extensions ts --exit",
    "test-pre": "mocha -r 'tests/index.spec.js'",
    "test-watch": "yarn mocha -r 'tests/index.spec.js' 'tests/**/*.spec.ts' --watch-extensions ts --watch -R spec",
    "doc": "tsoa swagger"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test-unit && yarn build"
    }
  },
  "devDependencies": {
    "@types/bluebird": "3.5.26",
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/chai-http": "3.0.5",
    "@types/debug": "4.1.2",
    "@types/express": "4.16.1",
    "@types/mocha": "5.2.6",
    "@types/node": "11.9.5",
    "axios": "0.18.0",
    "axios-mock-adapter": "1.16.0",
    "bluebird": "3.5.3",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-http": "4.2.1",
    "chai-subset": "1.6.0",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "mocha-junit-reporter": "1.18.0",
    "nodemon": "1.18.10",
    "nyc": "13.3.0",
    "sinon": "7.2.5",
    "sinon-chai": "3.3.0",
    "ts-node": "8.0.2",
    "tsoa-custom-decorators": "1.1.10-custom-decorators",
    "typescript": "3.3.3333",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1"
  },
  "dependencies": {
    "awilix": "4.2.0",
    "body-parser": "1.18.3",
    "celebrate": "9.0.2",
    "dotenv-safe": "6.1.0",
    "express": "4.16.4",
    "firebase-admin": "8.2.0",
    "glob": "7.1.3",
    "https": "1.0.0",
    "ramda": "0.26.1",
    "routing-controllers": "0.7.7",
    "swagger-ui-express": "4.0.2",
    "winston": "3.2.1"
  }
}
